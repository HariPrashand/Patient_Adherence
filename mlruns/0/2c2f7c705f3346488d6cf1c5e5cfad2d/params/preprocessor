ColumnTransformer(transformers=[('num',
                                 Pipeline(steps=[('imputer', SimpleImputer())]),
                                 Index(['Age', 'MedianIncome', 'TrialLengthWeeks', 'DoseLengthSeconds',
       'DoseDelayHours'],
      dtype='object')),
                                ('cat',
                                 Pipeline(steps=[('imputer',
                                                  SimpleImputer(strategy='most_frequent')),
                                                 ('onehot',
                                                  OneHotEncoder(handle_unknown='ignore'))]),
                                 Index(['Gender', 'Race', 'InsuranceType', 'HospitalizationPriorYear',
       'MSRelatedHospitalization', 'RelapsePriorYear', 'Disease',
       'TherapeuticArea', 'SpecialtyPharma', 'MicroReimbursements',
       'MedicationName', 'BrandName'],
      dtype='object'))])